{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "5",
          "edge": "12",
          "firefox": "18",
          "safari": "10",
          "ios": "10"
        }
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "."
        ]
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": "current"
            }
          }
        ],
        "@babel/preset-react"
      ]
    }
  },
  "comments": false,
  "minified": true
}
